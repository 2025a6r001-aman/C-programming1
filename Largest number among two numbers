#include<stdio.h>
int main(){
    int num1,num2;
    printf("Enter  first Number:");
    scanf("%d", &num1);
    printf("Enter  second Number");
    scanf("%d", &num2);
    if(num1>num2){
      printf("%d is largest number\n", num1);
   }
  else{
      printf("%d is largest number", num2);
   }
   return 0;
}
