#include<stdio.h>
int main(){
    char ch;
    printf("Enter character:");
    scanf("%c", &ch);
    if((ch>=65 && ch<=90)|| (ch>=97 && ch<=122))
      printf("%c is alphabet !!!", ch);
    else if (ch>=48 && ch<=57)
         printf("%c is digit !!!");
    else 
          printf("%c is symbol !!!");
    return 0;
}
